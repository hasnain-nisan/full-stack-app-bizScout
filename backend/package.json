{
   "name": "http-monitor",
   "version": "1.0.0",
   "description": "A full-stack application that monitors HTTP responses and displays them in real-time.",
   "main": "src/server.js",
   "scripts": {
      "start": "node src/server.js",
      "dev": "nodemon --exec ts-node src/server.ts",
      "build": "tsc",
      "lint": "eslint . --ext .ts",
      "lint:fix": "eslint . --ext .ts --fix",
      "test": "jest",
      "format": "prettier --write ."
   },
   "keywords": [
      "http",
      "monitor",
      "typescript",
      "express",
      "mongodb",
      "real-time"
   ],
   "author": "Hasnain Nisan",
   "license": "MIT",
   "dependencies": {
      "axios": "^1.4.0",
      "cors": "^2.8.5",
      "dotenv": "^16.0.3",
      "express": "^4.18.2",
      "mongoose": "^7.3.4",
      "node-cron": "^3.0.3",
      "socket.io": "^4.6.1",
      "uuid": "^11.0.5",
      "winston": "^3.17.0"
   },
   "devDependencies": {
      "@types/express": "^4.17.17",
      "@types/jest": "^29.5.14",
      "@types/mongoose": "^5.11.97",
      "@types/node": "^20.5.1",
      "@types/node-cron": "^3.0.11",
      "@types/supertest": "^6.0.2",
      "@typescript-eslint/eslint-plugin": "^6.5.1",
      "@typescript-eslint/parser": "^6.5.1",
      "eslint": "^8.46.0",
      "jest": "^29.7.0",
      "nodemon": "^3.0.1",
      "prettier": "^3.4.2",
      "supertest": "^7.0.0",
      "ts-jest": "^29.2.5",
      "ts-node": "^10.9.2",
      "typescript": "^5.3.3"
   },
   "engines": {
      "node": ">=16.0.0"
   }
}
